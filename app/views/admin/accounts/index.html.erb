<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>


<%= form_tag admin_accounts_path, :method => 'get', :id => "accounts_search" do %>
    <p>
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag "Search", :name => nil %>
    </p>
    <div id="accounts"><%= render 'accounts' %></div>
<% end %>

