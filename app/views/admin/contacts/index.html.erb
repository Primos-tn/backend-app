<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>


<%= form_tag admin_contacts_path, :method => 'get', :id => "contacts_search" do %>
    <p>
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag "Search", :name => nil %>
    </p>
    <div id="contacts"><%= render 'contacts' %></div>
<% end %>

