<p id="notice"><%= notice %></p>

<h1><%= t('Listing Users') %></h1>


<%= form_tag admin_accounts_path, :method => 'get', :id => "business_profile_search" do %>
    <p>
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag "Search", :name => nil %>
    </p>
    <div id="accounts"><%= render 'requests' %></div>
<% end %>

