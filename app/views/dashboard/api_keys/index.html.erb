<div class="row">
  <div class="col-md-12">
    <div class="card">
      <div class="header">
        <h4 class="title"><%= I18n.t('Api-keys') %></h4>
        <%= link_to I18n.t('New'), new_dashboard_hook_path %>
      </div>
      <div class="content table-responsive table-full-width">
        <table class="table table-striped">
          <thead>
          <th>Value</th>
          <th>Valid to</th>
          <th></th>
          </thead>
          <tbody>
      <% @api_keys.each do |apiKey| %>
          <tr>
            <td><%= apiKey.value %></td>
            <td><%= apiKey.valide_to %></td>
            <td><%= link_to 'Destroy',  dashboard_api_key_path(apiKey), method: :delete, data: {confirm: 'Are you sure?'} , class: 'danger'%></td>

          </tr>
      <% end %>
          </tbody>
        </table>
      <div class="text-center">
        <%= render 'form' %>
      </div>
      </div>
    </div>
  </div>

</div>
