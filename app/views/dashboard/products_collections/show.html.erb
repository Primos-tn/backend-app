<div class="card">
  <div class="header">
    <h4 class="title">
      <%= t('Editing Product') %>
      <%= link_to  dashboard_products_collections_path, class: 'pull-right' do %>
          <%= t('List') %>
      <% end %>
    </h4>
    <div class="content">
      <div class="row">
        <%= @products_collection.name %>
      </div>

      <div class="row">
        <h4><%= t('Products') %></h4>
        <%- @products_collection.products.each do |item| %>
          <div> <%= item.name %></div>
        <% end %>
      </div>
    </div>
  </div>
</div>