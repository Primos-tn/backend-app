<div class="card">
  <div class="header">
    <h4 class="title">
      <%= t('Import') %> : <%= @step %>
    </h4>
  </div>
  <div class="content">
    <%= form_tag import_dashboard_products_path, multipart: true do %>
        <div class="row">
          <div class="col-lg-12">
            <%= file_field_tag :file, class: "btn btn-info btn-fill btn-wd" %>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-12">
            <label><%= t('Import with header') %></label>
            <%= check_box_tag :with_header %>
          </div>
        </div>
        <div class="row">
          <div class="col-lg-12 text-center">
            <%= submit_tag 'import', class: "btn btn-info btn-fill btn-wd" %>
          </div>
        </div>
    <% end %>
  </div>
</div>