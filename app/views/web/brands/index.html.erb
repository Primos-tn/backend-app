<% content_for :scripts do %>
    <% render partial: 'shared/requires_map_libs' %>
    <%= javascript_include_tag 'components/user/brands/index', "data-turbolinks-track" => true %>
    <script>
      $('input').iCheck({
        checkboxClass: 'icheckbox_square-grey',
        radioClass: 'iradio_square-grey'
      });
    </script>
<% end %>

<section class="parallax-window" data-parallax="scroll" data-image-src="assets/brands/hotels_bg.jpg" data-natural-width="1400" data-natural-height="470">
