<template>
  <div class="col-md-6 col-sm-6 wow zoomIn" data-wow-delay="0.6s">
    <div class="tour_container">
      <div class="ribbon_3"><span>Top rated</span></div>
      <div class="img_container">
        <a href="single_restaurant.html">
          <img src="{{App.Helpers.getMediaUrl(cover.thumb.url)}}" width="100%"  class="img-responsive brand-image" alt="Image">
          <div class="short_info">
            <i class="{{category ? category.icon_class_name : ''}}"></i> {{App.Helpers.localize(category, 'name')}}<span class="price"><sup><i class=" icon-shop-1"></i></sup>{{stores.length}}</span>
          </div>
        </a>
      </div>
      <div class="tour_title">
        <h3><strong>{{name}}</strong></h3>
        <div class="rating">
          <i class="icon-smile voted"></i><i class="icon-smile voted"></i><i class="icon-smile voted"></i><i class="icon-smile voted"></i><i class="icon-smile"></i>
